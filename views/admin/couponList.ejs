    <script>

function coupontatus(couponId, couponstatusElementId) {
    console.log(couponstatusElementId);
    Swal.fire({
        title: 'Are you sure?',
        text: 'You want to change the Coupon Status!',
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#3085d6',
        cancelButtonColor: '#d33',
        confirmButtonText: 'Yes'
    }).then((result) => {
        if (result.isConfirmed) {
            $.ajax({
                url: '/admin/coupon-status/' + couponId,
                method: 'get',
                // data: {
                //     couponId: couponId
                // },
                success: (response) => {
                    Swal.fire(
                        'Done!',
                        'Coupon Status has been Changed.',
                        'success'
                    );
console.log(response);
console.log(response.coupon);
                    
                    const coupontatusElement = document.getElementById(couponstatusElementId);
                    if (response.coupon.isListed) {
                        coupontatusElement.innerHTML = 'Listed'
                    } else{
                        coupontatusElement.innerHTML = 'UnListed'
                    }
                },
                error: (error) => {
                    Swal.fire(
                        'Error!',
                        'An error occurred while blocking the user.',
                        'error'
                    );
                }
            });
        }
    });
}



