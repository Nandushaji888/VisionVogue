        <section class="content-main">
            <div class="row">
                <div class="col-9">
                    <div class="content-header">
                        <h2 class="content-title">Add New Coupon</h2>
              
                    </div>
                </div>
                <form action="/admin/add-coupon" method="post">
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="mb-4">
                                <label for="product_title" class="form-label">Coupon Code</label>
                                <input type="text" name="couponCode" required placeholder="Type here" class="form-control" id="product_title">
                            </div>
                            <div class="row gx-3">
                                <div class="col-md-4  mb-3">
                                    <label for="product_sku" class="form-label">Discount % </label>
                                    <input name="discount"  type="text" max="100" min="0" required placeholder="Type here" class="form-control" id="product_sku">
                                </div>
                                <div class="col-md-4  mb-3">
                                    <label for="product_color" class="form-label">Max Discount Amount</label>
                                    <input name="maxDiscount"  type="text" required placeholder="Type here" class="form-control" id="product_color">
                                 
                                </div>
                                <div class="col-md-4  mb-3">
                                    <label class="form-label">isListed</label>
                                    <select class="form-select" name="isListed">
                                        <option value='true'>True</option>
                                        <option value='false'>False</option>
                                    </select>
                                </div>
                                
                            </div>
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">Description</label>
                                <input type="text" name="description" required placeholder="Type here" class="form-control" id="product_brand">
                            </div>
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">Min Order Price</label>
                                <input name="minOrderPrice" type="text" required placeholder="Type here" class="form-control" id="product_brand">
                            </div>
                           
                            <% if (typeof message !=='undefined' ) { %>
                                <p style="color: rgba(239, 10, 10, 0.836);">
                                    <%= message %>
                                        <p>
                                            <% } %>
                            <button type="submit" class="btn btn-md rounded font-sm hover-up">Publich</button>
                        </div>
                       
                    </div> <!-- card end// -->
                 
                </div>
            </form>
            </div>
