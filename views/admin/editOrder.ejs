        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order detail</h2>
                    <!-- <p>Details for Order ID:<%=order._id%></p> -->
                </div>
            </div>
            <div class="card">
                <header class="card-header">
                    <div class="row align-items-center">
                        <!-- <div class="col-lg-6 col-md-6 mb-lg-0 mb-15">
                            <span>
                                <i class="material-icons md-calendar_today"></i> <b>Wed, Aug 13, 2022, 4:34PM</b>
                            </span> <br>
                            <small class="text-muted">Order ID: <%=order._id%></small>
                        </div> -->
                        <div class="col-lg-6 col-md-6 ms-auto text-md-end">
                            <form action="/admin/change-order-status" method="post">
                                <span>Order Status : </span>
                            <select class="form-select d-inline-block mb-lg-0 mb-15 mw-200" name="status">
                                <option <%if(order.orderStatus=== "PENDING") { %> selected <% } %> value="PENDING">PENDING</option>
                                <option <%if(order.orderStatus=== "PLACED") { %> selected <% } %> value="PLACED">PLACED</option>
                                <option <%if(order.orderStatus=== "CANCELLED") { %> selected <% } %> value="CANCELLED">CANCELLED</option>
                                <option <%if(order.orderStatus=== "SHIPPED") { %> selected <% } %> value="SHIPPED">SHIPPED</option>
                                <option <%if(order.orderStatus=== "DELIVERED") { %> selected <% } %> value="DELIVERED">DELIVERED</option>
                                <option <%if(order.orderStatus=== "RETURNED") { %> selected <% } %> value="RETURNED">RETURNED</option>
                              <!-- <option value="PENDING">PENDING</option>
                                <option value="PLACED">PLACED</option>
                                <option value="CANCELLED">CANCELLED</option>
                                <option value="DELIVERED">DELIVERED</option>
                                <option value="RETURNED">RETURNED</option> -->
                            </select>
                            <input type="hidden" name="id" id="" value="<%=order._id%>">
                            <button type="submit" class="btn btn-primary">Save</button>
                            <!-- <a class="btn btn-primary" href="#">Save</a> -->
                        </form>
                            <!-- <a class="btn btn-secondary print ms-2" href="#"><i class="icon material-icons md-print"></i></a> -->
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="row mb-50 mt-20 order-info-wrap">
                        <div class="col-md-4">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="text-primary material-icons md-person"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Customer</h6>
                                    <p class="mb-1">
                                        <%=customer.customerId.name%> <br>  <%=customer.customerId.email%> <br>  <%=customer.customerId.phone%>
                                    </p>
                                    
                                </div>
                            </article>
                        </div> <!-- col// -->
                        <!-- <div class="col-md-4">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="text-primary material-icons md-local_shipping"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Order info</h6>
                                    <p class="mb-1">
                                        Shipping:  <br> Pay method: card <br> Status: new
                                    </p>
                                    
                                </div>
                            </article>
                        </div> col// -->
                        <div class="col-md-4">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="text-primary material-icons md-place"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Deliver to</h6>
                                    <p class="mb-1">
                                        <%=order.shippingAddress.customerName%> <br><%=order.shippingAddress.addressLine1%>,<%=order.shippingAddress.city%><br>
                                        <%=order.shippingAddress.city%>, <%=order.shippingAddress.zipcode%>  <br> <%=order.shippingAddress.email%> <br> <%=order.shippingAddress.phone%> 
                                    </p>
                                    
                                </div>
                            </article>
                        </div> <!-- col// -->
                        <div class="col-md-4">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="text-primary material-icons md-place"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Payment Details</h6>
                                    
                                    <p class="mb-1">
                                        Payment Type : <%= order.paymentMethod%>
                                    </p>
                                    <%if(order.paymentMethod == 'razorpay'){%>
                                        <%if(order.paymentOrderId ){%>

                                    <p class="mb-1">
                                        Transaction ID :<%=String(order.paymentOrderId).slice(6,20)%> 
                                    </p>
                                    <%}%>
                                    <%}%>
                                </div>
                            </article>
                        </div> <!-- col// -->

                        <%if(order.paymentStatus =="REFUNDED"){%>
                            <div class="col-md-4">
                                <article class="icontext align-items-start">
                                    <span class="icon icon-sm rounded-circle bg-primary-light">
                                        <i class="text-primary material-icons md-place"></i>
                                    </span>
                                    <div class="text">
                                        <h6 class="mb-1">Order Return</h6>
                                        
                                        <p class="mb-1">
                                            Return reason : <%= order.returnReason%>
                                        </p>
                                       
                                    </div>
                                </article>
                            </div> <!-- col// -->
                            <%}%>

                        
                    </div> <!-- row // -->
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th width="10%">Date</th>
                                            <th width="15%">Order ID</th>
                                            <th width="20%">Product</th>
                                            <th width="10%">Unit Price</th>
                                            <th width="10%">Quantity</th>
                                            <th width="15%">Payment Status</th>
                                            <th width="15%">Order Status</th>
                                            <th width="10%" class="text-end">Total</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% let subtotal = 0%>

                                        <%for(let i=0; i < order.products.length; i++){%>
                                        <tr>
                                            <td> <%=String(order.createdAt).slice(4,16)%> </td>
                                            <td> ODR<%= order._id.toString().slice(0, 13) %> </td>

                                            <td>
                                                <a class="itemside" href="#">
                                                    <div class="left">
                                                        <img src="/assets2/imgs/products/<%=order.products[i].productId.image[0]%>" width="40" height="40" class="img-xs" alt="Item">
                                                    </div>
                                                    <div class="info"><%=order.products[i].productId.name%> </div>
                                                </a>
                                            </td>
                                            <td>₹ <span><%=order.products[i].productId.price%></span></td>
                                            <td> <%=order.products[i].quantity%> </td>
                                            <td> <%=order.paymentStatus%> </td>
                                            <td> <%=order.orderStatus%> </td>
                                            <td class="text-end">₹<span><%=order.products[i].productId.price
                                                *order.products[i].quantity %></span> </td>
                                                <% subtotal=subtotal+(order.products[i].productId.price *
                                                order.products[i].quantity )%>
                                        </tr>
                                        <%}%>
                                        <tr>
                                            <td colspan="12">
                                                <article class="float-end">
                                                    <dl class="dlist">
                                                        <dt>Subtotal:</dt>
                                                        <dd>₹<span> <%=subtotal%></span></dd>
                                                    </dl>
                                                    <dl class="dlist d-flex">
                                                        <dt>Discount:</dt>
                                                        <dd>₹<span><%= subtotal - order.paidAmount  %></span></dd>
                                                    </dl>
                                                    <dl class="dlist">
                                                        <dt>Shipping cost:</dt>
                                                        <dd>00.00</dd>
                                                    </dl>
                                                    <dl class="dlist">
                                                        <dt>Grand total:</dt>
                                                        <dd> <b class="h5">₹<span> <%=order.paidAmount%></b> </dd>
                                                    </dl>
                                             
                                                </article>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div> <!-- table-responsive// -->
                            <!-- <a class="btn btn-primary" href="page-orders-tracking.html">View Order Tracking</a> -->
                        </div> <!-- col// -->
                       
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> ©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="/assets2/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="/assets2/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="/assets2/js/vendors/select2.min.js"></script>
    <script src="/assets2/js/vendors/perfect-scrollbar.js"></script>
    <script src="/assets2/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="/assets2/js/main.js" type="text/javascript"></script>
</body>

</html>