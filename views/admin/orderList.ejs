        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <p>Lorem ipsum dolor sit amet.</p>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
         
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="dataTable">
                            <thead>
                                <tr>
                                    <th scope="col">Date</th>
                                    <th>ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <%if(locals.order) {%>
                                    <%order.sort((a, b) => new Date(b.createdAt) - new Date(a.createdAt));%>

                                <%for(let i = order.length-1; i>=0; i--){%>
                                <tr>
                                    <td><%=String(order[i].createdAt).slice(4,16)%></td>
                                    <td><%=order[i].orderId %>
                                    </td>
                                    <td><b><%=order[i].customerId.name%></b></td>
                                    <td><%=order[i].customerId.email%></td>
                                    <td>₹ <%=order[i].paidAmount%></td>
                                    <%if(order[i].orderStatus === "PENDING"){%>
                                    <td><span class="badge rounded-pill alert-warning"><%=order[i].orderStatus%></span></td>
                                    <%}else if(order[i].orderStatus === "PLACED"){%>
                                        <td><span class="badge rounded-pill alert-success"><%=order[i].orderStatus%></span></td>
                                        <%}else if(order[i].orderStatus === "CANCELLED") {%>
                                            <td><span class="badge rounded-pill alert-danger"><%=order[i].orderStatus%></span></td>
                                         <%}else if(order[i].orderStatus === "DELIVERED") {%>
                                            <td><span class="badge rounded-pill alert-primary"><%=order[i].orderStatus%></span></td>
                                            
                                            <%}else if(order[i].orderStatus === "RETURNED") {%>
                                               <td><span class="badge rounded-pill alert-info"><%=order[i].orderStatus%></span></td>
                                            <%}else if(order[i].orderStatus === "SHIPPED") {%>
                                               <td><span class="badge rounded-pill alert-secondary"><%=order[i].orderStatus%></span></td>

                                            <%}%>
                                    <td class="text-end">
                                        <!-- <a href="/admin/order-details/<%=order[i]._id%>" class="btn btn-md rounded font-sm">Detail</a> -->
                                        <button class="rounded btn-info"><a style="color: black;" href="/admin/order-details/<%=order[i]._id%>" class="btn btn-sm rounded font-sm">Edit</a></button>
                                        
                                        <!-- <div class="dropdown">
                                            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" href="#">View detail</a>
                                                <a class="dropdown-item" href="#">Edit info</a>
                                                <a class="dropdown-item text-danger" href="#">Delete</a>
                                            </div>
                                        </div> dropdown //end -->
                                    </td>
                                </tr>
                                <%}%>

                                
                                <%}else{%>
                               
                                    <h2>No Data Found</h2>
                                    <%}%>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->

